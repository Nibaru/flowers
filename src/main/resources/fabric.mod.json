    "sources": "https://github.com/Nibaru/flowers",
    "issues": "https://github.com/Nibaru/flowers/issues"
  },
  "license": "LGPL-3",
  "icon": "assets/flowers/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "games.twinhead.flowers.client.FlowersClient"
    ],
    "main": [
      "games.twinhead.flowers.Flowers"
